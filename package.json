{
  "name": "SBHoot",
  "version": "1.0.0",
  "description": "A trivia game",
  "main": "index.js",
  "unpkg": "dist/aframe-websocket-component.min.js",
  "scripts": {
    "dev": "nodemon --watch index.js --watch dist/student.js --watch dist/instructor.js --watch app.js --watch common.pug --watch student.pug --watch instructor.pug --exec 'node app.js' ",
    "lint": "semistandard -v | snazzy",
    "ghpages": "ghpages",
    "test": "karma start ./tests/karma.conf.js",
    "test:firefox": "karma start ./tests/karma.conf.js --browsers Firefox",
    "test:chrome": "karma start ./tests/karma.conf.js --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SBUtltmedia/SBHoot.git"
  },
  "keywords": [
    "trivia",
    "quiz",
    "game",
    "sbhoot",
    "websocket"
  ],
  "author": "TLTMediaLab@connect.stonybrook.edu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SBUtltmedia/SBHoot/issues"
  },
  "homepage": "https://github.com/SBUtltmedia/SBHoot",
  "devDependencies": {
    "aframe": "^0.9.2",
    "browserify": "^16.2.3",
    "budo": "^11.6.2",
    "chai": "^4.2.0",
    "chai-shallow-deep-equal": "*",
    "ghpages": "0.0.10",
    "karma": "^6.4.1",
    "karma-browserify": "^6.0.0",
    "karma-chai-shallow-deep-equal": "*",
    "karma-chrome-launcher": "^2.2.0",
    "karma-env-preprocessor": "*",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "randomcolor": "^0.5.4",
    "semistandard": "^13.0.1",
    "shelljs": "^0.8.3",
    "shx": "^0.3.2",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "snazzy": "^8.0.0",
    "uglify-es": "*",
    "webpack": "^4.35.2"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  },
  "dependencies": {
    "csv-parse": "^4.4.3",
    "csvtojson": "^2.0.10",
    "express": "^4.17.1",
    "formidable": "^1.2.1",
    "mongodb-stitch": "^3.2.24",
    "mysql": "^2.17.1",
    "node-libcurl": "^2.0.2",
    "pm2": "^3.5.1",
    "pug": "^2.0.4",
    "socket.io": "*",
    "socketio-file-upload": "^0.7.0",
    "sqlstring": "^2.3.1"
  }
}
