<div id="signout" style="display: none;">
  <button id="signout-button">Sign Out</button>
  <p id="greeting"></p>
  <div id="gameCreation">
    <h3>Create a game!</h3>
    <h5>Room ID: <input type="text" id="roomId" value="yourClass"><br>
      <button id="makeGame" type="button" name="button">Create Game!</button>
    </h5>
    <h3>Rejoin a game!</h3>
    <h5 id="rejoin"><ul id="previousGames"/><h5>
  </div>
</div>

<div id="gameManagement" style="display:none;">
  <h3>Please join room <span id="gameName"/>:</h3>
  <ul id="playerList"></ul>
  <table id="playerResults" style="display: none;"/>
  <button id="downloadReport" type="button" name="button">Download Game Report</button>
  <button id="openGame" type="button" name="button" style="display: none;">Open</button>
  <button id="closeGame" type="button" name="button">Close</button>
  <button id="deleteGame"type="button" name="button">Delete</button>
  <button id="startGame" type="button" name="button" style="display: none;">Start</button>
  <div id="file_drop" dropzone="copy" title="drop files for upload">Drop Question File to Start</div>
</div>
<span id="GameError"></span>
<div id="dialog" title="Error">
  <p id="dialogText"></p>
</div>
</body>
    <script src="dist/siofu.js"></script>
<script src="dist/instructor.js"></script>
</html>
