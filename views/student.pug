<div id="signout" style="display: none;">
  <button id="signout-button">Sign Out</button>
  <p id="greeting"></p>
  <h3>Join a game!</h3>
  <h5>Nickname: <input id="nickname" type="text" name="nickname" value="bill">
  <br/>Room ID: <input id="roomId" type="text" name="" value="yourClass"><br/>
    <button id="joinGame" type="button" name="button">Join Game!</button><span id="joinGameError"></span>
  </h5>
</div>

<div id="waitingRoom" style="display: none;">
  <h5>Joined room <id="roomGiven" span></span></h5>
  <h6>Currently in room:</h6>
  <ul id="playerList"></ul>
  <button id="leaveRoom" type="button" name="button">Leave Room</button>
</div>

<div id="stage" style="display: none;" class="screen">
  <div id="header">
    <div id="code"></div>
    <div id="question"></div>
    <div id="timer"></div>
  </div>
  <div id="answer_0" class="answer"></div>
  <div id="answer_1" class="answer"></div>
  <div id="answer_2" class="answer"></div>
  <div id="answer_3" class="answer"></div>
  <div id="submit"></div>
  <span style="left:1%;">Score: <span id="pointCounter"/></span>
  <br/>
  <div id="topRanked_0" class="topRanked"></div>
  <div id="topRanked_1" class="topRanked"></div>
  <div id="topRanked_2" class="topRanked"></div>
  <div id="topRanked_3" class="topRanked"></div>
  <div id="topRanked_4" class="topRanked"></div>
</div>

<div id="dialog" title="Error">
  <p id="dialogText"></p>
</div>
</body>
 <script src="dist/client.js"></script>
</html>
